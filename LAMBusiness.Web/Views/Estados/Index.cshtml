@model LAMBusiness.Shared.Aplicacion.Filtro<List<LAMBusiness.Shared.Catalogo.Estado>>

@{
    ViewData["Title"] = "Estados";
}

<div class="modal-content border-0 box-shadow">
    <div class="modal-header pb-0 align-items-baseline">
        <div class="ms-3">
            <h3 class="modal-title" id="exampleModalLabel">Estados</h3>
            <ul class="breadcrumb bg-transparent p-0 small">
                <li class="breadcrumb-item"><a asp-action="Catalogo" asp-controller="Menu">Catálogos</a></li>
                <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Estados">Estados</a></li>
                <li class="breadcrumb-item active text-muted">Lista</li>
            </ul>
        </div>
        <div class="d-flex align-items-center">
            <a class="btn-help text-secondary me-3" href="/" onclick="help('ViewIndex', event);">
                <i class="fa fa-question-circle"></i>
            </a>
            <a asp-action="Catalogo" asp-controller="Menu" class="btn-close me-md-1" aria-label="Close"></a>
        </div>
    </div>
    <div class="modal-body pt-4" data-module="Estados">
        <div class="row px-md-3">
            <div class="col-9 col-md-8 pr-0 pr-md-3">
                <div class="input-group mb-3 search">
                    <input type="search" id="SearchInput" class="form-control" placeholder="Buscar..." onkeypress="search(event);">
                    <button class="btn btn-light border" type="button" onclick="search(event);"><i class="fa fa-search"></i></button>
                </div>
            </div>
        </div>
        <div>
            <partial name="_AddRowsNextAsync" model="Model" />
        </div>
    </div>
</div>